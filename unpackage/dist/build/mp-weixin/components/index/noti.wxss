.noti-wrapper{width:100%;height:60rpx;padding:0 30rpx;margin:50rpx 0 0;box-sizing:border-box}.noti-wrapper .noti-box{width:100%;height:100%;border-radius:10rpx;background-color:#f0f9ff;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center}.noti-wrapper .noti-box .noti-icon{width:28rpx;height:28rpx;margin:0 20rpx 0 20rpx;background:url(data:image/png;base64,) no-repeat;background-size:100%}.noti-wrapper .noti-box .swiper-box{height:100%;width:100%}.noti-wrapper .noti-box .swiper-box .noti-swiper{height:100%;width:100%}.noti-wrapper .noti-box .swiper-box .noti-swiper .swiper-item{width:100%;height:20rpx;line-height:60rpx;color:#0177bf;font-size:24rpx}