.canteen-list-page{padding:24rpx 30rpx 20rpx;box-sizing:border-box}.canteen-list-page .canteen-list-page-title{margin-bottom:16rpx;font-size:30rpx;font-weight:700;color:#333}.canteen-list-page .tips{position:relative;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center;width:100%;height:80rpx;font-size:24rpx;color:#999;text-align:center}.canteen-list-page .tips ._span{background-color:#fff;padding:0 20rpx;z-index:10}.canteen-list-page .tips .bg-line{position:absolute;content:"";z-index:1;top:50%;-webkit-transform:translateY(-50%);transform:translateY(-50%);width:50%;height:1rpx;background-color:#999}