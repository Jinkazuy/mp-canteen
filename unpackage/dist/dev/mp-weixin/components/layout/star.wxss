.star-wrapper {
  position: relative;
  width: 116rpx;
  height: 20rpx;
  margin-right: 10rpx;
}
.star-wrapper .star-fill {
  position: absolute;
  left: 0;
  top: 0;
  height: 100%;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 116rpx 20rpx;
}
.star-wrapper .star-nor {
  position: absolute;
  left: 0;
  top: 0;
  height: 100%;
  width: 100%;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}

