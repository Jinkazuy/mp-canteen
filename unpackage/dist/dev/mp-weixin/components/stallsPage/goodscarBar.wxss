.car-bar {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  height: 96rpx;
  background-color: #4d4b4a;
}
.car-bar .car-bar-left-box {
  position: relative;
  height: 100%;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  background-color: #4d4b4a;
}
.car-bar .car-bar-left-box .car-icon-wrapper {
  position: absolute;
  left: 24rpx;
  bottom: 4rpx;
  width: 112rpx;
  height: 112rpx;
  box-sizing: border-box;
  border-radius: 50%;
  padding: 12rpx;
  background-color: #4d4b4a;
}
.car-bar .car-bar-left-box .car-icon-wrapper .car-icon-nor,
.car-bar .car-bar-left-box .car-icon-wrapper .car-icon-act {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  width: 100%;
  height: 100%;
  border-radius: 50%;
  -webkit-transition: all 0.3s;
  transition: all 0.3s;
}
.car-bar .car-bar-left-box .car-icon-wrapper .car-icon-nor ._i,
.car-bar .car-bar-left-box .car-icon-wrapper .car-icon-act ._i {
  width: 40rpx;
  height: 40rpx;
  background-position: center;
  background-size: 100%;
  background-repeat: no-repeat;
  -webkit-transition: all 0.3s;
  transition: all 0.3s;
}
.car-bar .car-bar-left-box .car-icon-wrapper .car-icon-nor {
  background-color: #2b343c;
}
.car-bar .car-bar-left-box .car-icon-wrapper .car-icon-nor ._i {
  background-image: url(data:image/png;base64,);
}
.car-bar .car-bar-left-box .car-icon-wrapper .car-icon-act {
  background-color: #fff;
}
.car-bar .car-bar-left-box .car-icon-wrapper .car-icon-act ._i {
  background-image: url(data:image/png;base64,);
}
.car-bar .car-bar-left-box .car-icon-wrapper .arr-num {
  position: absolute;
  right: 10rpx;
  top: 10rpx;
  width: 30rpx;
  height: 30rpx;
  background-color: #fc5858;
  border-radius: 50%;
  font-size: 18rpx;
  color: #fff;
  line-height: 30rpx;
  text-align: center;
}
.car-bar .car-bar-left-box .payment-amount {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 160rpx;
  right: 50rpx;
  height: 100%;
  line-height: 96rpx;
  font-size: 36rpx;
  font-weight: 700;
  color: #fff;
}
.car-bar .payment-btn-nor,
.car-bar .payment-btn-act {
  -webkit-flex-basis: 200rpx;
          flex-basis: 200rpx;
  height: 100%;
  font-size: 30rpx;
  font-weight: 700;
  line-height: 96rpx;
  text-align: center;
}
.car-bar .payment-btn-nor {
  background-color: #737373;
  color: #fff;
}
.car-bar .payment-btn-act {
  background-color: #3ecc78;
  color: #fff;
}

