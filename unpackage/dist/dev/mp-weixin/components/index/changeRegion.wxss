.change-region {
  width: 100%;
  height: 186rpx;
  padding: 30rpx 74rpx 38rpx;
  box-sizing: border-box;
}
.change-region ._ul {
  height: 100%;
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.change-region ._ul ._li {
  width: 66rpx;
  height: 100%;
  position: relative;
}
.change-region ._ul ._li image {
  width: 66rpx;
  height: 66rpx;
}
.change-region ._ul ._li .food {
  margin-top: 10rpx;
  width: 50rpx;
  height: 50rpx;
}
.change-region ._ul ._li ._div {
  position: absolute;
  bottom: 0;
  text-align: center;
  font-size: 24rpx;
  color: #444;
}

