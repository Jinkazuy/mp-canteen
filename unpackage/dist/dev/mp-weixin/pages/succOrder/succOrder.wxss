.succ-rder {
  padding: 0 30rpx;
  box-sizing: border-box;
}
.succ-rder .stalls-tit {
  height: 120rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border-bottom: 1rpx solid #eee;
}
.succ-rder .stalls-tit .img-box {
  width: 100rpx;
  height: 100rpx;
  border-radius: 10rpx;
  margin-right: 20rpx;
  background-color: #eee;
}
.succ-rder .stalls-tit .stalls-name {
  height: 100rpx;
  font-size: 36rpx;
  line-height: 100rpx;
  font-weight: 700;
  color: #666;
}
.succ-rder .stalls-tit .stalls-name ._span {
  font-size: 26rpx;
  margin-left: 20rpx;
}
.succ-rder .stalls-toatl-price-wrapper {
  height: 90rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  border-bottom: 1rpx solid #eee;
  margin-bottom: 30rpx;
}
.succ-rder .stalls-toatl-price-wrapper .order-time {
  font-size: 24rpx;
  color: #666;
}
.succ-rder .stalls-toatl-price-wrapper .stalls-toatl-price {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
}
.succ-rder .stalls-toatl-price-wrapper .stalls-toatl-price .sum-tip {
  font-size: 24rpx;
  color: #666;
}
.succ-rder .stalls-toatl-price-wrapper .stalls-toatl-price .stalls-toatl-price-num {
  font-size: 36rpx;
  color: #666;
  font-weight: 700;
}
.succ-rder .estimated-time {
  text-align: center;
  font-weight: 700;
  font-size: 24rpx;
  color: #666;
  margin-bottom: 24rpx;
}
.succ-rder .take-meals-code {
  text-align: center;
}
.succ-rder .take-meals-code ._span {
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 36rpx;
  color: #666;
  font-size: 24rpx;
  margin: 0 auto;
  border: 1rpx solid #0177bf;
  border-radius: 10rpx;
}

