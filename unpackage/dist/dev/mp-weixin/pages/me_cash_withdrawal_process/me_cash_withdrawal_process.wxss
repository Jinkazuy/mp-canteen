.me-cash-withdrawal-process {
  padding: 90rpx 50rpx 0;
  box-sizing: border-box;
}
.me-cash-withdrawal-process .step-wrapper {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 0 0 80rpx 66rpx;
  border-bottom: 1rpx solid #999;
  box-sizing: border-box;
}
.me-cash-withdrawal-process .step-wrapper .left-lines-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 8rpx 0;
  box-sizing: border-box;
  width: 36rpx;
}
.me-cash-withdrawal-process .step-wrapper .left-lines-box .round-o,
.me-cash-withdrawal-process .step-wrapper .left-lines-box .round-t {
  width: 14rpx;
  height: 14rpx;
  border-radius: 50%;
}
.me-cash-withdrawal-process .step-wrapper .left-lines-box .round-o {
  background-color: #0177bf;
}
.me-cash-withdrawal-process .step-wrapper .left-lines-box .round-t {
  background-color: #bfbfbf;
}
.me-cash-withdrawal-process .step-wrapper .left-lines-box .lines-icon {
  width: 36rpx;
  height: 36rpx;
}
.me-cash-withdrawal-process .step-wrapper .left-lines-box .lines-icon ._img {
  width: 100%;
  height: 100%;
}
.me-cash-withdrawal-process .step-wrapper .left-lines-box .lines-o,
.me-cash-withdrawal-process .step-wrapper .left-lines-box .lines-t {
  width: 2rpx;
  margin-left: 2rpx;
  box-sizing: border-box;
}
.me-cash-withdrawal-process .step-wrapper .left-lines-box .lines-o {
  height: 80rpx;
  border-left: 2rpx dashed #0177bf;
}
.me-cash-withdrawal-process .step-wrapper .left-lines-box .lines-t {
  height: 120rpx;
  border-left: 2rpx dashed #bfbfbf;
}
.me-cash-withdrawal-process .step-wrapper .text-group {
  margin-left: 26rpx;
}
.me-cash-withdrawal-process .step-wrapper .text-group .text-o,
.me-cash-withdrawal-process .step-wrapper .text-group .text-t,
.me-cash-withdrawal-process .step-wrapper .text-group .text-h,
.me-cash-withdrawal-process .step-wrapper .text-group .text-r {
  font-size: 30rpx;
  line-height: 1;
}
.me-cash-withdrawal-process .step-wrapper .text-group .text-o {
  color: #666;
  margin-bottom: 78rpx;
}
.me-cash-withdrawal-process .step-wrapper .text-group .text-t {
  color: #0177bf;
  margin-bottom: 20rpx;
}
.me-cash-withdrawal-process .step-wrapper .text-group .text-h {
  color: #999;
  margin-bottom: 66rpx;
}
.me-cash-withdrawal-process .step-wrapper .text-group .text-r {
  color: #999;
}
.me-cash-withdrawal-process .cash-withdrawal-info {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  margin: 20rpx 0 400rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 30rpx;
}
.me-cash-withdrawal-process .cash-withdrawal-info .cash-withdrawal-info-tit {
  color: #999;
}
.me-cash-withdrawal-process .cash-withdrawal-info .cash-withdrawal-info-num {
  color: #333;
  font-size: 700;
}
.me-cash-withdrawal-process .submit-btn {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.me-cash-withdrawal-process .van-button--primary {
  margin: 0 auto !important;
  width: 300rpx;
  color: #0177bf !important;
  border-radius: 10rpx !important;
  background-color: #dcdcdc !important;
  border: none !important;
  font-size: 30rpx !important;
  line-height: 36rpx !important;
  -webkit-transition: all 0.4s !important;
  transition: all 0.4s !important;
}

